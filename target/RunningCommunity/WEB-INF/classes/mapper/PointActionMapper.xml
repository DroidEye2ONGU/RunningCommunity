<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="droideye.mapper.PointActionMapper">
    <select id="queryPointActionByActionName" resultType="pointaction">
        SELECT * FROM POINTACTION
        WHERE ACTIONNAME=#{actionName}
    </select>

    <select id="queryPointActionById" resultType="pointaction">
        SELECT * FROM POINTACTION
        WHERE ID=#{id}
    </select>
</mapper>